<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.adjh.multiflexapi.mapper.UserMapper">

    <!--로그인-->
    <select id="login" resultType="Users">
        SELECT t1.*
        FROM USERS t1
        WHERE UserID = #{userId}
    </select>
